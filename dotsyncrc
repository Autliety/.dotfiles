# Location of your dotfiles in $HOME
DOTFILES=.dotfiles

# dotfiles to symlink
#
# srcfile:dstfile
# dstfile will be '.$srcfile' by default (with path removed)
#
[files]
#dotsyncrc
bashrc
bash_profile
bin:bin
gitconfig
ssh
vim
ideavimrc

[endfiles]

