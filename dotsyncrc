# Location of your dotfiles in $HOME
DOTFILES=.dotfiles

# dotfiles to symlink
#
# srcfile:dstfile
# dstfile will be '.$srcfile' by default (with path removed)
#
[files]
#dotsyncrc
bashrc
bash_profile
bin:bin
config
gitconfig
ssh
vim
ideavimrc
[endfiles]

# hostname except options
#
# hostname                  type=masterhost, ANY or NONE        files=dotfile:tolink,another:tolink
[hosts]
Auly-Mac.local              git=NONE
Clock-2.local                 git=NONE
[endhosts]

